<% if obj.errors.any? %>
  <div id="error_explanation">
    <h3><%= I18n.t('activerecord.errors.template.header.other', model: obj.model_name.human, count: obj.errors.count) %></h3>
    <ul>
      <% obj.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
